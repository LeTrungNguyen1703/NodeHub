# .env file
#
# Example values only â€” replace with your real secrets before use.

# Database Configuration
MYSQL_DATABASE=note_db_example
MYSQL_USER=note_user_example
MYSQL_PASSWORD=example_note_password
MYSQL_ROOT_PASSWORD=example_root_password

# Port Configuration
# The port on your local machine (localhost)
MYSQL_LOCAL_PORT=3307
# The port inside the Docker container (usually 3306)
MYSQL_DOCKER_PORT=3306

# --- Keycloak Config ---
# We map Keycloak to 8180 to avoid conflict with Spring Boot (8080)
KEYCLOAK_PORT=8180
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
# Replace with your Keycloak client credentials
KEYCLOAK_CLIENT_ID=auction-api
KEYCLOAK_CLIENT_SECRET=example_keycloak_client_secret

SPRING_SECURITY_OAUTH2_RESOURCESERVER_JWT_ISSUER_URI=http://localhost:8180/realms/auction-system
SPRING_SECURITY_OAUTH2_RESOURCESERVER_JWT_JWK_SET_URI=http://localhost:8180/realms/auction-system/protocol/openid-connect/certs

# --- Google OAuth Config ---
GOOGLE_CLIENT_ID=example_google_client_id
GOOGLE_CLIENT_SECRET=example_google_client_secret

# --- Cloudinary Config ---
CLOUDINARY_CLOUD_NAME=example_cloud_name
CLOUDINARY_API_KEY=example_api_key
CLOUDINARY_API_SECRET=example_api_secret

# --- Docker Hub Config ---
DOCKER_HUB_USERNAME=example_username
DOCKER_HUB_REPO=example_repo
DOCKER_HUB_SECRET=example_dockerhub_token

# --- Backend Config ---
BACKEND_PORT=8080

# Vite backend port (uncomment if used)
# VITE_BACKEND_PORT=8080